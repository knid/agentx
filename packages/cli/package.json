{
  "name": "@knid/agentx",
  "version": "0.1.2",
  "description": "The package manager for AI agents powered by Claude Code",
  "type": "module",
  "bin": {
    "agentx": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "execa": "^9.0.0",
    "@clack/prompts": "^0.9.0",
    "zod": "^3.23.0",
    "yaml": "^2.6.0",
    "ofetch": "^1.4.0",
    "chalk": "^5.4.0",
    "tar": "^7.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^3.0.0",
    "typescript": "^5.7.0",
    "@types/node": "^22.0.0",
    "@types/tar": "^6.1.0"
  },
  "keywords": [
    "ai",
    "agents",
    "claude",
    "claude-code",
    "mcp",
    "model-context-protocol",
    "cli",
    "package-manager",
    "agent-marketplace",
    "llm"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/knid/agentx"
  },
  "homepage": "https://agentx-web.vercel.app",
  "bugs": {
    "url": "https://github.com/knid/agentx/issues"
  },
  "author": "knid",
  "engines": {
    "node": ">=18"
  }
}
